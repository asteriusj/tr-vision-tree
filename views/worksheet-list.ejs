<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
  </head>
  <body>
    <h1><%= title %></h1>

    <ul>
      <% for(var i=0; i<worksheets.length; i++) { %>
      <li>
        <a href="/spreadsheets/<%=spreadsheetId %>/<%=worksheets[i].id %>">
          <%=worksheets[i].title %>
        </a>
      </li>
      <% } %>
    </ul>
  </body>
</html>